import random
user = input("What is your name?: ")
r = random.randrange(1, 100)

def pokemon(user, r):
    r = random.randrange(1, 100)
    print(r)

    if r < 25:
       print("Aww UNCLE please? I want to challenge this GYM!")
    elif r < 60:
       print(" Uncle, I'll go back to verdanturf")
    elif r < 80:
       print ("oh wow a pokemon!")
    else:
       print("OH", user, "its me WALLY")

pokemon(user, r)
