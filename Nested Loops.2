#include<iostream>
using namespace std;

int main() {

	for (int i = 0; i < 5; i++) { //affects vertical number
		for (int j = 1; j <= 4; j++) { // affects horizontal number
			cout << j;// affects the pattern letter / number
		}
		cout << endl;
	}

	for (int i = 0; i < 6; i++) { //affects vertical number
		for (int j = 0; j <= 10; j++) { // affects horizontal number
			cout << "*";// affects the pattern letter / number
		}
		cout << endl;
	}

}
