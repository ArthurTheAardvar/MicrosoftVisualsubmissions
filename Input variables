#include <iostream>
using namespace std;

int main() {

	char name1;
	char name2;
	cout << "whats is your fist and last initial" << endl;
	cin >> name1;
	cin >> name2; 
	cout << name1 << name2 << endl;

	int age;
	cout << "enter id" << endl;
	cin >> age;
	cout << "Your id is " << age << endl;

	float money;
	cout << "How much money do you have?" << endl;
	cin >> money;
	cout << "You have " << money << endl;

	
}
