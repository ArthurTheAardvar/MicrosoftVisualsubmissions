#include<iostream>
using namespace std;

int main() {
	for (int i = 0; i < 8; i++) { //affects vertical number
		for (int j = 0; j < 3; j++) { // affects horizontal number
			cout << "*";// affects the pattern letter/number
		}
		cout << endl;
	}
	for (int i = 0; i < 4; i++) { //affects vertical number
		for (int j = 0; j < 9;j++) { // affects horizontal number
			cout << "*";// affects the pattern letter/number
		}
		cout << endl;
	}
	for (int i = 0; i < 7; i++) { //affects vertical number
		for (int j = 0; j < 1; j++) { // affects horizontal number
			cout << "012";//affects the pattern letter/number
		}
		cout << endl;
	}
	for (int i = 0; i < 4; i++) { //affects vertical number
		for (int j = 0; j < 9; j++) { // affects horizontal number
			cout << i;// affects the pattern letter / number
		}
		cout << endl;
	}
	
}
